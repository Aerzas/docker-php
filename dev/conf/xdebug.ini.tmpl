[xdebug]
xdebug.default_enable = $PHP_XDEBUG_DEFAULT_ENABLE
xdebug.cli_color = 1
xdebug.coverage_enable = $PHP_XDEBUG_COVERAGE_ENABLE
xdebug.file_link_format = $PHP_XDEBUG_FILE_LINK_FORMAT
xdebug.max_nesting_level = $PHP_XDEBUG_MAX_NESTING_LEVEL
xdebug.profiler_enable = $PHP_XDEBUG_PROFILER_ENABLE
xdebug.profiler_enable_trigger = $PHP_XDEBUG_PROFILER_ENABLE_TRIGGER
xdebug.profiler_output_dir = $PHP_XDEBUG_LOG_DIR/profiler/
xdebug.remote_autostart = $PHP_XDEBUG_REMOTE_AUTOSTART
xdebug.remote_log = $PHP_XDEBUG_REMOTE_LOG_PATH
xdebug.remote_enable = $PHP_XDEBUG_REMOTE_ENABLE
xdebug.remote_connect_back = $PHP_XDEBUG_REMOTE_CONNECT_BACK
xdebug.remote_host = $PHP_XDEBUG_REMOTE_HOST
xdebug.remote_port = $PHP_XDEBUG_REMOTE_PORT
xdebug.trace_enable = $PHP_XDEBUG_TRACE_ENABLE
xdebug.trace_enable_trigger = $PHP_XDEBUG_TRACE_ENABLE_TRIGGER
xdebug.trace_output_dir = $PHP_XDEBUG_LOG_DIR/trace/
xdebug.idekey= $PHP_XDEBUG_IDEKEY
